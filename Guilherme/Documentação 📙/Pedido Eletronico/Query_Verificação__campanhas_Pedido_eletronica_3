-- VERIFICAR CAMPANHAS GERADAS
    -- CAMPANHA 
    SELECT 	* 
    FROM 	TEL_CAMPANHA tc 
    WHERE 	ID IN (522,523,524,525,526,527)
    ORDER BY ID DESC;

    -- CLIENTES 
    SELECT 	*
    FROM 	TEL_CLIENTE tc 
--    WHERE ID_CAMPANHA IN (481,482,483,484,485,486)
    WHERE ID_CAMPANHA IN (522,523,524,525,526,527)
    
    -- PRODUTOS 
    SELECT 	*
    FROM 	TEL_CAMPANHA_PRODUTO_DESCONTO tcpd 
    WHERE 	ID_CAMPANHA IN (522,523,524,525,526,527)

    -- TELEFONES QUE SERÃO ENVIADOS
    SELECT  * 
    FROM    TEL_CLIENTE_TELEFONES_CAMPANHA
    WHERE   ID_CAMPANHA IN (522,523,524,525,526,527)
    
-- 
-- ENVIO 
    -- SQL TELEVENDAS
    SELECT *	
    FROM TEL_ENVIO_TELEVENDAS tet 
    WHERE tet."ds_email" IN (522,523,524,525,526,527)
    -- NOTIFICAÇÃO: 145	Dep. Televendas - Envio de promoção positivação clientes
    -- NOTIFICAÇÃO: 164	Dep. Televendas - Envio de promoção positivação clientes
    